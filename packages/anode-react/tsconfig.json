{
  "compilerOptions": {
    "module": "nodenext",
    "target": "esnext",
    "types": [],
    "paths": {
      // "anode": ["../../src/anode.ts"]
    },
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "strict": true,
    "jsx": "react-jsx",
    "verbatimModuleSyntax": true,
    "isolatedModules": true,
    "noUncheckedSideEffectImports": true,
    "moduleDetection": "force",
    "skipLibCheck": true,
    "noEmit": true
  }
}
